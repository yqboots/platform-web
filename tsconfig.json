{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@yq/core": [
        "../dist/@yq/core"
      ],
      "@yq/core/*": [
        "../dist/@yq/core/*"
      ],
      "@yq/components": [
        "../dist/@yq/components"
      ],
      "@yq/components/*": [
        "../dist/@yq/components/*"
      ],
      "@yq/extensions": [
        "../dist/@yq/extensions"
      ],
      "@yq/extensions/*": [
        "../dist/@yq/extensions/*"
      ],
      "@yq/docs": [
        "../dist/@yq/docs"
      ],
      "@yq/docs/*": [
        "../dist/@yq/docs/*"
      ],
      "@yq/showcase": [
        "../dist/@yq/showcase"
      ],
      "@yq/showcase/*": [
        "../dist/@yq/showcase/*"
      ]
    }
  }
}